{
	"name": "props-mark",
	"displayName": "props-mark",
	"description": "自动生成props文档",
	"version": "0.0.1",
	"publisher": "qiaolufei",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:props-mark.helloWorld"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "props-mark.helloWorld",
				"title": "Hello World"
			}
		],
		"keybindings": [
			{
				"command": "props-mark.helloWorld",
				"key": "shift+q",
				"mac": "shift+q"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.63.0",
		"@vscode/test-electron": "^2.0.3",
		"acorn": "^8.7.0",
		"eslint": "^8.6.0",
		"glob": "^7.2.0",
		"mocha": "^9.1.3",
		"typescript": "^4.5.4"
	}
}
